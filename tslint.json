{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint:latest", "tslint-react"],
  "jsRules": {},
  "rules": {
    "interface-name": [true, "never-prefix"],
    "quotemark": [true, "single", "avoid-escape"],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}
